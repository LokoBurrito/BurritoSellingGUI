local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
getgenv().Theme = "DarkTheme"
local Window = Library.CreateLib("Burrito's DHC Selling Essentials", getgenv().Theme)
local Tab = Window:NewTab("SELLING GUIS")
local Section = Tab:NewSection("Crostide GUI's")
Section:NewButton("Crostide Seller GUI", "Track all players DHC", function()
 loadstring(game:HttpGet("https://raw.githubusercontent.com/Crostide/cdhc/main/gui"))()
end)
Section:NewButton("Crostide DHC Counter", "Counts the DHC on ground", function()
 loadstring(game:HttpGet("https://raw.githubusercontent.com/Crostide/Counter/main/Cash"))()
end)
local Tab = Window:NewTab("Credits")
Section:NewLabel("Made By: LokoBurrito on roblox")
Section:NewLabel("(or lokoburrito. on discord)")
Section:NewLabel("UI Made by: xHeptc")
local Section = Tab:NewSection("Settings")
Section:NewKeybind("GUI Keybind", "Keybind to turn the GUI on and off", Enum.KeyCode.F, function()
 Library:ToggleUI()
end)
local Tab = Window:NewTab("Update Log")
Section:NewLabel("Update 2/20/24 (V1.2)")
Section:NewLabel("Added: GUI Keybinds")
Section:NewLabel("Added: Custom Cash Counter ")
print("Thanks for using Burrito's Seller GUI!")
